# Copy this file to .env.local and fill in real secrets before running the pipeline.
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=YOUR_NEO4J_PASSWORD
NEO4J_DATABASE=neo4j
NEO4J_AUTH=neo4j/YOUR_NEO4J_PASSWORD
# NEO4J_HTTP_PORT=7474            # Optional: override host HTTP port exposed by docker compose
# NEO4J_BOLT_PORT=7687            # Optional: override host Bolt port exposed by docker compose
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"
OPENAI_PROJECT="YOUR_OPENAI_PROJECT"
OPENAI_EMBEDDING_MODEL="text-embedding-3-small"  # fallback if EMBEDDING_MODEL is unset
OPENAI_MODEL="gpt-5-mini"  # optional fallback: gpt-4o-mini
# OPENAI_TEMPERATURE="0.3"   # optional: adjust response variability (ignored for gpt-5* models)
# OPENAI_EMBEDDING_DIMENSIONS="1024"             # optional: override embedding dimensions
QDRANT_URL="http://localhost:6333"  # optional: only needed for Qdrant export workflows
QDRANT_API_KEY=""                   # optional: leave blank for local/dev Qdrant
EMBEDDING_API_KEY="YOUR_EMBEDDING_API_KEY"
EMBEDDING_API_BASE_URL="YOUR_EMBEDDING_API_BASE_URL"  # required for local embeddings (e.g., http://localhost:20010/v1)
# EMBEDDING_DIMENSIONS="1024"  # optional: override embedding dimensions
EMBEDDING_MODEL="YOUR_EMBEDDING_MODEL"  # local embedding model name
# OPENAI_ALLOW_INSECURE_BASE_URL="true"           # allow http EMBEDDING_API_BASE_URL for local/dev only
# EMBEDDING_TIMEOUT_SECONDS=10  # optional: request timeout in seconds
# EMBEDDING_MAX_RETRIES=3       # optional: retry count
INDEX_NAME=YOUR_INDEX_NAME  # default: text_embeddings
FULLTEXT_INDEX_NAME=YOUR_FULLTEXT_INDEX_NAME  # default: chunk_text_fulltext
FULLTEXT_LABEL=YOUR_FULLTEXT_LABEL  # default: Chunk
FULLTEXT_PROPERTY=YOUR_FULLTEXT_PROPERTY  # default: text
FULLTEXT_READY_ATTEMPTS=YOUR_FULLTEXT_READY_ATTEMPTS  # default: 10
FULLTEXT_READY_DELAY=YOUR_FULLTEXT_READY_DELAY  # default: 3
MCP_AUTH_REQUIRED=YOUR_MCP_AUTH_REQUIRED  # default: true; set false for local-only runs without OAuth
MCP_BASE_URL=YOUR_MCP_BASE_URL  # default: http://localhost:8080
# MCP_SERVER_HOST=0.0.0.0
# MCP_SERVER_PORT=8080
# MCP_SERVER_PATH=/mcp
# MCP_PUBLISHED_PORT=8080         # Optional: override host port exposed by docker compose
# MCP_STATIC_TOKEN=dev-smoke-token  # Optional: enable static bearer auth for local smoke tests only
GOOGLE_OAUTH_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID
GOOGLE_OAUTH_CLIENT_SECRET=YOUR_GOOGLE_OAUTH_CLIENT_SECRET
GOOGLE_OAUTH_REQUIRED_SCOPES=YOUR_GOOGLE_OAUTH_REQUIRED_SCOPES  # default: openid,https://www.googleapis.com/auth/userinfo.email
HYBRID_RETRIEVAL_QUERY_PATH=YOUR_HYBRID_RETRIEVAL_QUERY_PATH  # default: queries/hybrid_retrieval.cypher
